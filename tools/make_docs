#!/bin/sh

echo -e "\n=== Compiling documentation ===\n"
if [ ! -d ../docs ]
then
  mkdir ../docs
fi
cp overview.txt LRG_Logo.png ../Addons

naturaldocs -r -i "../Addons" -xi "../Addons/Main/Functions/AIS" -o HTML "../docs" -p "lrg-fundamentals" -s Default lrg
rm ../Addons/{overview.txt,LRG_Logo.png}